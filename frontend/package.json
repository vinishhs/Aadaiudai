{
  "name": "aadaiudai-frontend",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:renderer": "cd renderer && npm run dev",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "cd renderer && npm run build && npm run export",
    "build:electron": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "AADAIUDAI Premium E-Commerce Desktop App",
  "dependencies": {
    "electron-is-dev": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "wait-on": "^8.0.2"
  }
}